        xmlns:local="clr-namespace:Applicazione_Utente"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnAccedi" Content="Accedi" HorizontalAlignment="Left" Margin="263,211,0,0" VerticalAlignment="Top" Width="202" Click="btnAccedi_Click"/>
